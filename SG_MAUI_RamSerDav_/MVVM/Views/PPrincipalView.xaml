<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SG_MAUI_RamSerDav_.MVVM.ViewModels.PPrincipalView"
             Title="PPrincipalView">
    <Grid HorizontalOptions="CenterAndExpand" Margin="0,25,0,0" VerticalOptions="CenterAndExpand" ColumnDefinitions="*,*">
        <ImageButton Grid.Column="0" Source="usuarios.png" Padding="10" WidthRequest="250" HeightRequest="250" Command="{Binding btnIrGestionUsuariosCommand}" Margin="25,25,25,25"/>
        <ImageButton Grid.Column="1" Source="salir.png" Padding="10" WidthRequest="250" HeightRequest="250" Command="{Binding btnSalirCommand}" Margin="25,25,25,25"/>
    </Grid>
</ContentPage>